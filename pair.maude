
fmod PAIR{X :: TRIV, Y :: TRIV} is
    sort Pair{X, Y} .
    op [_,_] : X$Elt Y$Elt -> Pair{X, Y} .
    op 1st_ : Pair{X, Y} -> X$Elt .
    op 2nd : Pair{X, Y} -> Y$Elt .
    op 2nd__ : X$Elt Y$Elt -> Y$Elt .
    var A : X$Elt .
    var B : Y$Elt .
    eq 1st[A,B] = A .
    eq 2nd([A,B]) = B .
    eq 2nd A B = B .
endfm

fmod M is
  pr PAIR{Nat, String} .
endfm

red 1st[0,"A"] .
red 2nd 0 "A" .
