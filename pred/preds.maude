load ../model/model.maude
in model-checker.maude .

mod PREDS is
 pr MODEL .
 inc SATISFACTION .
 subsort Sys < State .
 
 op configured : -> Prop .
 
 var S : Sys .

 vars SCRIPT_CON : Bag{Connection} .
 vars SCRIPT-VM CLOUD-VM : Bag{Machine} .

 eq  cloud(CLOUD-VM, SCRIPT_CON) script(SCRIPT-VM, SCRIPT_CON) design(SCRIPT_CON) S |= configured = true .

